.qq-gallery .qq-upload-drop-area, .qq-upload-extra-drop-area { position: absolute; top: 0px; left: 0px; width: 100%; height: 100%; min-height: 30px; z-index: 2; background: rgb(249, 249, 249); border-radius: 4px; text-align: center; }

